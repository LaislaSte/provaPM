<div class="container">
        <h1>Tela de Edição</h1>
        <form action="/editar" method="post">
            {{#each agendamento}}
            <input type="hidden" class="form-control hidden" id="id" value="{{dataValues.id}}" name="id" />
            <div class="form-group">
                <label for="nome">Nome:</label>
                <input type="text" class="form-control" name="nome" id="nome" placeholder="Digite o nome" value="{{dataValues.nome}}">
            </div>
            <div class="form-group">
                <label for="endereco">Endereço:</label>
                <input type="text" class="form-control" name="endereco" id="endereco" placeholder="Digite o endereço" value="{{dataValues.endereco}}">
            </div>
            <div class="form-group">
                <label for="bairro">Bairro:</label>
                <input type="text" class="form-control" name="bairro" id="bairro" placeholder="Digite o bairro" value="{{dataValues.bairro}}">
            </div>
            <div class="form-group">
                <label for="cep">CEP:</label>
                <input type="text" class="form-control" name="cep" id="cep" placeholder="Digite o CEP" value="{{dataValues.cep}}">
            </div>
            <div class="form-group">
                <label for="cidade">Cidade:</label>
                <input type="text" class="form-control" name="cidade" id="cidade" placeholder="Digite a cidade" value="{{dataValues.cidade}}">
            </div>
            <div class="form-group">
                <label for="estado">Estado:</label>
                <input type="text" class="form-control" name="estado" id="estado" placeholder="Digite o estado" value="{{dataValues.estado}} ">
            </div>
            <button type="submit" class="btn btn-primary" >Salvar</button>
             {{/each}}
        </form>
    </div>